  function switcheroo() {
    campaignTrail_temp.running_mate_image_url = 'https://cdn.discordapp.com/attachments/896483055575896074/1093653968804130866/Downloader.la-642f3cb1147d3.jpg';
    campaignTrail_temp.running_mate_last_name = 'Griffin';
    campaignTrail_temp.candidate_image_url = 'https://cdn.discordapp.com/attachments/896483055575896074/1093652168646610944/Downloader.la-642f3cb7aa0da.jpg';
    campaignTrail_temp.candidate_last_name = 'Tower';
    e.candidate_json[0].fields.first_name = "John"
    e.candidate_json[0].fields.last_name = "Tower"        
}
function tunnel(new_q){
    e = campaignTrail_temp;
    return e.questions_json[e.questions_json.map(f=>f.pk).indexOf(new_q)];
}  
// my reasoning? hump becomes a disloyal whore. trys to get the peace plank added to separate himself
// from LBJ, that leads to LBJ helicoptering into the convention and seizing it (real plan irl)
function switcheroo3() {
    e.candidate_json[1].fields.first_name = "Lyndon"
    e.candidate_json[1].fields.last_name = "Johnson"      
    
}

function switcheroo4() {
    campaignTrail_temp.running_mate_image_url = 'https://cdn.discordapp.com/attachments/896483055575896074/1094351418271465593/image.png';
    campaignTrail_temp.running_mate_last_name = 'Ashbrook';
    campaignTrail_temp.candidate_image_url = 'https://cdn.discordapp.com/attachments/896483055575896074/1094351418623803432/image.png';
}
